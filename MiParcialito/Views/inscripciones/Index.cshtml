@model IEnumerable<MiParcialito.Models.inscripciones>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Inscribir Nueva", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cursos.nombreCurso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.user.email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cursos.nombreCurso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idInscripcion }) |
            @Html.ActionLink("Details", "Details", new { id=item.idInscripcion }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idInscripcion })
        </td>
    </tr>
}

</table>
